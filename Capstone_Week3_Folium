    "# loop through\n",
    "for lat, lng, label in zip(launch_sites_df.Lat, launch_sites_df.Long, launch_sites_df['Launch Site']):\n",
    "    coordinate = [lat, lng]\n",
    "    circle1 = folium.Circle(coordinate, radius=1000, color='#d35400', fill=True).add_child(folium.Popup(label))\n",
    "    #folium.Circle(coordinate, radius=1000, color='#d35400', fill=True).add_child(folium.Popup(label))\n",
    "    marker1 = folium.map.Marker(coordinate, icon=DivIcon(icon_size=(20,20),icon_anchor=(0,0), html='<div style=\"font-size: 12; color:#d35400;\"><b>%s</b></div>' % label, ))\n",
    "    \n",
    "# show map\n",
    "    site_map.add_child(circle1)\n",
    "    site_map.add_child(marker1)\n",
    "site_map"
   ]
