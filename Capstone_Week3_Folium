       "        marker_3c6e84a809b1529c39769918b6771236.bindPopup(popup_f308e5cfb985d93a205d2c564b6e45f7)\n",
       "        ;\n",
       "\n",
       "        \n",
       "    \n",
       "    \n",
       "            var marker_8e666297d104c12922c837dc8c5999a3 = L.marker(\n",
       "                [28.56319718, -80.57682003],\n",
       "                {}\n",
       "            ).addTo(marker_cluster_503a57f7acac1174dbd8d6700eca67d4);\n",
       "        \n",
       "    \n",
       "            var icon_46aef69ce295c9ba64918c2971fbff9e = L.AwesomeMarkers.icon(\n",
       "                {&quot;extraClasses&quot;: &quot;fa-rotate-0&quot;, &quot;icon&quot;: &quot;info-sign&quot;, &quot;iconColor&quot;: &quot;red&quot;, &quot;markerColor&quot;: &quot;white&quot;, &quot;prefix&quot;: &quot;glyphicon&quot;}\n",
       "            );\n",
       "            marker_8e666297d104c12922c837dc8c5999a3.setIcon(icon_46aef69ce295c9ba64918c2971fbff9e);\n",
       "        \n",
       "    \n",
       "        var popup_262d35932211162d8165f029698581c4 = L.popup({&quot;maxWidth&quot;: &quot;100%&quot;});\n",
